{
  "name": "Task Scheduler",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "A Task Scheduler Extension for Chrome",
  "permissions": ["storage", "activeTab"],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png",
    "256": "icon.png"
  }
}
